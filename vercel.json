{
  "version": 2,
  "builds": [
    {
      "src": "Client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Client/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/Client/index.html"
    }
  ],
  "outputDirectory": "Client/dist"
}